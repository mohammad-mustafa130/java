class ReverseString {
	public static void main(String[] args) {
		String s = "Lucknow";
		System.out.println("Orignal String: "+ s);
		char[] ch = s.toCharArray();
		int i = 0;
		int j = ch.length - 1;
		while(i < j) {
			char temp = ch[i];
			ch[i] = ch[j];
			ch[j] = temp;
			i++;
			j--;
		}
		String rs = new String(ch);
		System.out.println("Reverse String: "+rs);
	}
}
